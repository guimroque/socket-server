{
  "name": "node-typescript-api",
  "version": "1.0.0",
  "engines": {
    "node": ">21.5.0"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "test": "sudo chmod +x ./scripts/init_test.sh && ./scripts/init_test.sh",
    "start": "pm2-runtime start ./build/server/index.js",
    "build": "tsc --project . && tscpaths -p tsconfig.json -s ./src -o ./build",
    "copyFiles": "copyfiles --error --up 1 src/**/*.html build",
    "postbuild": "yarn run copyFiles",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "1.5.1",
    "bcrypt": "5.1.0",
    "body-parser": "1.20.2",
    "bsafe": "0.0.49",
    "cheerio": "1.0.0-rc.12",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.17.1",
    "express-joi-validation": "5.0.0",
    "fuels": "0.74.0",
    "joi": "17.4.0",
    "morgan": "1.10.0",
    "patch-package": "8.0.0",
    "pg": "8.5.1",
    "pm2": "5.3.0",
    "reflect-metadata": "0.1.13",
    "socket.io": "4.7.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.29",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@trivago/prettier-plugin-sort-imports": "2.0.2",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "9.0.2",
    "@types/morgan": "1.9.2",
    "@types/node": "20.6.0",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "5.2.0",
    "jest": "29.7.0",
